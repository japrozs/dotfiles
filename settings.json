// Theme color Classes can be found here!!!
//https://code.visualstudio.com/api/references/theme-color
{
    "workbench.startupEditor": "newUntitledFile",
    "editor.formatOnSave": true,
    "terminal.integrated.defaultProfile.osx": "fish",
    "editor.fontSize": 17,
    "explorer.confirmDelete": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "explorer.confirmDragAndDrop": false,
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace, Menlo",
    "liveServer.settings.donotVerifyTags": true,
    "java.semanticHighlighting.enabled": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "prettier.tabWidth": 4,
    "color-highlight.languages": ["*", "!c", "!cpp", "!h"],
    "editor.minimap.enabled": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.class": "default"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.fontSize": 16,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "json.schemas": [
        {
            "name": "vscode-expo-manifest",
            "url": "file:///Users/pritpalsinghsaini/Library/Application%20Support/Code/User/globalStorage/bycedric.vscode-expo/manifest-42.0.0.json",
            "fileMatch": ["app.json", "app.config.json"]
        }
    ],
    "editor.accessibilitySupport": "off",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "C_Cpp.default.includePath": ["${default}", "$[workspaceFolder}/include"],
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<esc>"]
        }
    ],
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": ["comment.line.double-slash.cpp"],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    }
}
