{
    "workbench.startupEditor": "newUntitledFile",
    "editor.formatOnSave": true,
    "terminal.integrated.defaultProfile.osx": "fish",
    "editor.fontSize": 17,
    "explorer.confirmDelete": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "explorer.confirmDragAndDrop": false,
    "editor.fontFamily": "'DejaVu Sans Mono', Menlo, Monaco, 'Courier New', monospace, Menlo, Segoe UI Emoji",
    "liveServer.settings.donotVerifyTags": true,
    "java.semanticHighlighting.enabled": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "prettier.tabWidth": 4,
    "color-highlight.languages": ["*", "!c", "!cpp", "!h"],
    "color-highlight.markerType": "dot-before",
    "editor.minimap.enabled": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.class": "default"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.fontSize": 16,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "clang-format.executable": "/usr/local/opt/llvm/bin/clang-format",
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "C_Cpp.default.browse.path": [
        "${default}",
        "$[workspaceFolder}/include",
        "/opt/local/include",
        "/usr/local/opt/freetype/include/freetype2",
        "/usr/local/include/",
        "${workspaceFolder}",
        "/usr/local/include/SDL2",
        "/opt/homebrew/Cellar/glfw/3.3.9/include",
        "/opt/homebrew/Cellar/glm/0.9.9.8/include/",
        "/Users/japroz/VulkanSDK/1.3.268.1/macOS/include"
    ],
    "C_Cpp.default.includePath": [
        "${default}",
        "$[workspaceFolder}/include",
        "/opt/local/include",
        "/usr/local/opt/freetype/include/freetype2",
        "/usr/local/include/",
        "${workspaceFolder}",
        "/usr/local/include/SDL2",
        "/usr/local/Cellar/glew/2.2.0_1/include",
        "/usr/local/Cellar/glfw/3.3.6/include"
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<esc>"]
        }
    ],
    "editor.rulers": [80],
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": ["comment.line.double-slash.cpp"],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "cmake.configureOnOpen": false,
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "editor.renderWhitespace": "none",
    "editor.inlineSuggest.enabled": true,
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "npm.exclude": ["**/xpacks/**"],
    "haskell.manageHLS": "GHCup",
    "[python]": {
        "editor.formatOnType": true
    },
    "window.commandCenter": true,
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.tsdk": "node_modules/typescript/lib",
    "typst-lsp.exportPdf": "never",
    "arduino.useArduinoCli": true,
    "workbench.colorTheme": "One Dark Pro Monokai Darker"
}
